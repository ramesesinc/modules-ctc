import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.common.*
import com.rameses.services.extended.*

class IndividualCTCPaymentService  {
	
	@DataContext("ctc_individual")
	def ctcEm;

	@ProxyMethod 
	def postCashReceipt(def params) {

		params.txnmode = "ONLINE";
		params.dtissued = params.receiptdate;
		params.receiptid = params.objid;
		params.voided = 0;
		params.each { k,v->
			println k+"="+v;
		}
		ctcEm.create( params );
		//throw new Exception("Saving ctc");

	}


}